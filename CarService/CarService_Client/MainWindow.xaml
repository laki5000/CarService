<Window x:Class="CarService_Client.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:CarService_Client"
        mc:Ignorable="d"
        Title="B &amp; L Car Service" Height="450" Width="815"
        MinHeight="450" MinWidth="815"
        MaxHeight="450" MaxWidth="815">
    <Grid>
        <Button x:Name="ButtonMainCustomer" HorizontalAlignment="Center" Click="HandleButtonMainCustomer" Content="Customer" FontSize="20" Width="815">
            <Button.Background>
                <ImageBrush ImageSource="raw\car.png" Stretch="None"/>
            </Button.Background>
        </Button>

    </Grid>
</Window>
